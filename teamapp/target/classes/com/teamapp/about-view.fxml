<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.teamapp.controller.AboutController"
      alignment="CENTER" spacing="15" prefWidth="400" prefHeight="250">

    <padding>
        <Insets top="20" bottom="20" left="20" right="20"/>
    </padding>

    <Label text="Team Members" style="-fx-font-size: 16px; -fx-font-weight: bold;"/>

    <VBox fx:id="teamList" alignment="CENTER" spacing="10">
    </VBox>

    <Button text="Close" prefWidth="100" onAction="#closeWindow"/>
</VBox>